@model My_Portfolio_.Net_Core_6._0.DAL.Entities.Skill
@{
    ViewData["Title"] = "Add";
    Layout = "~/Views/Layout/Index.cshtml";
}

<div class="card">
	<div class="card-header">
		<div class="card-title">Add Skill</div>
	</div>
	<div class="card-body">
		<form method="post">
			<label>Title</label>
			<input type="text" asp-for="SkillTitle" class="form-control" />
			<br />
			<label>Value</label>
			<input type="text" asp-for="SkillValue" class="form-control" />
			<br />
			<button class="btn btn-primary">SAVE</button>
		</form>
	</div>
</div>

